{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "startServerCommand": "npm run build && npm run preview",
            "url": [
                "http://localhost:4321/",
                "http://localhost:4321/ta",
                "http://localhost:4321/services/category/popular-destinations"
            ],
            "settings": {
                "preset": "desktop"
            }
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:best-practices": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 1
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}